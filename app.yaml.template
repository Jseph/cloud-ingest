runtime: python
env: flex
service: api
entrypoint: gunicorn -e INGEST_CONFIG_PATH=ingestwebconsole.{}_settings -b :$PORT --chdir webconsole/backend main:APP

runtime_config:
  python_version: 2

skip_files:
- (?!^webconsole.*|^cloud-functions.*|^requirements.txt)
- ^webconsole/frontend.*
- ^cloud-functions/node_modules.*
- ^app.*yaml.*
- ^app_engine_config_generator\.py
